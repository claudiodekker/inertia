{
  "scripts": {
    "build:driver": "npm --prefix ../ install && npm run --prefix ../ build:cjs",
    "build:library": "npm --prefix ../../inertia install && npm run --prefix ../../inertia build:cjs",
    "build:tests": "webpack",
    "build": "npm run build:driver && npm run build:library && npm run build:tests",
    "ci": "npm install && npm run build && npm run test",
    "debug": "start-server-and-test 'node app/server.js' http://localhost:13714 'cypress open'",
    "test": "start-server-and-test 'node app/server.js' http://localhost:13714 'cypress run'"
  },
  "devDependencies": {
    "body-parser": "^1.19.0",
    "cypress": "^5.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "start-server-and-test": "^1.11.5",
    "vue": "^2.0.0",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
